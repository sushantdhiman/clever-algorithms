{
    "rules": {
        "indent": [
            2,
            2
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "no-new-symbol": [
            0
        ],
        "no-self-assign": [
            0
        ],
        "no-unused-labels": [
            0
        ],
        "comma-spacing": [
          2,
          {
            "before": false,
            "after": true
          }
        ],
        "arrow-parens": [
          2,
          "always"
        ],
        "no-console": [
          0
        ]
    },
    "env": {
        "node": true,
        "es6": true
    },
    "extends": "eslint:recommended"
}
